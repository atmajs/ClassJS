include
	.inject(
		'/src/util/object.js'
	)
	.done(function(){
				
		UTest({
					
			'check object': function(){
				
				var error,
					expect = {
						username: 'string',
						password: 'string',
						'?age': 'number',
						'?joker': {
							'x_joker': 'number',
							'?y_joker': 'number'
						}
					};
				
				error = obj_validate({
					username: 'foo',
					password: 'baz'
				}, expect);
				eq_(error, null);
				
				'> miss property'
				error = obj_validate({
					username: 'foo'
				}, expect);
				has_(error, {
					property: 'password'
				});
				
				'> type missmatch'
				error = obj_validate({
					username: 20,
					password: 'baz'
				}, expect);
				has_(error, {
					property: 'username'
				});
				
				error = obj_validate({
					username: 'foo',
					password: 'baz',
					age: 'qux'
				}, expect);
				has_(error, {
					property: 'age'
				});
				
				'> miss subproperty'
				error = obj_validate({
					username: 'foo',
					password: 'baz',
					joker: {} 
				}, expect);
				has_(error, {
					property: 'joker.x_joker'
				});
				
				error = obj_validate({
					username: 'foo',
					password: 'baz',
					joker: {
						x_joker: 'hello'
					} 
				}, expect);
				has_(error, {
					property: 'joker.x_joker'
				});
				
				'> invalid optional subproperty type'
				error = obj_validate({
					username: 'foo',
					password: 'baz',
					joker: {
						x_joker: 5,
						y_joker: 'baz'
					} 
				}, expect);
				has_(error, {
					property: 'joker.y_joker'
				});
				
				'> valid'
				error = obj_validate({
					username: 'foo',
					password: 'baz',
					joker: {
						x_joker: 5,
						y_joker: 7
					} 
				}, expect);
				eq_(error, null);
				
				'> array property: valid'
				expect = {
					collection: [ {_id: 'string'}]
				}
				error = obj_validate({
					collection: [
						{ _id: '4343' },
						{ _id: '4343' },
						{ _id: '4343' },
					]
				}, expect);
				eq_(error, null);
				
				'> array property: invalid'
				error = obj_validate({
					collection: [
						{ _id: '4343' },
						{ _id: '4343' },
						{ _id: 4343 },
					]
				}, expect);
				has_(error, {
					property: 'collection.2._id'
				});
				
				'> array'
				expect = [ {_id: 'string' }]
				error = obj_validate([
					{ _id: '4343' },
					{ _id: '4343' },
				], expect);
				eq_(error, null);
				
				
				'> regexp - valid'
				expect = { rgx: /^\w+$/ }
				error = obj_validate({
					rgx: 'asd92'
				}, expect);
				eq_(error, null);
				
				'> regexp - invalid'
				expect = { rgx: /^\w+$/ }
				error = obj_validate({
					rgx: 'as-d92'
				}, expect);
				has_(error, {
					property: 'rgx'
				});
				
				
				'> unexpected property'
				expect = { '-_id': null }
				error = obj_validate({
					_id: 'baz'
				}, expect);
				has_(error, {
					property: '_id'
				});
				
				'> function validation'
				expect = {
					'username': function(value){
						return value === 'baz'
					}
				}
				'> fn valid'
				error = obj_validate({
					username: 'baz'
				}, expect)
				eq_(error, null);
				
				'> fn invalid'
				error = obj_validate({
					username: 'foo'
				}, expect)
				has_(error, {
					property: 'username'
				});
			},
			
			'!strict': function(){
				var error,
					expect = {
						'?foo': 'string',
						'?joker': {
							'x': 'number'
						}
					};
				
				error = obj_validate({ baz: null}, expect, true);
				has_(error, {
					property: 'baz'
				});
				
				//> invalid subproperty `x`
				error = obj_validate({
					joker: {
						y: 5
					}
				}, expect, true);
				has_(error, {
					property: 'joker.x'
				});
				
				//> unexpected subproperty `y`
				error = obj_validate({
					joker: {
						x: 1,
						y: 2
					}
				}, expect, true);
				has_(error, {
					property: 'joker.y'
				});
				
				//> ok
				error = obj_validate({
					foo: 'foo_string',
					joker: {
						x: 1
					}
				}, expect, true);
				eq_(error, null);
			}
		})
		
	});